{
  "permissions": {
    "allow": [
      "Bash(echo:*)",
      "Bash(source:*)",
      "Bash(curl:*)",
      "Bash(python3:*)",
      "WebFetch(domain:inspect.aisi.org.uk)",
      "Bash(uv sync:*)",
      "Bash(uv venv:*)",
      "Bash(uv pip install:*)",
      "Bash(chmod:*)",
      "Bash(inspect eval --help:*)",
      "Bash(pkill:*)",
      "Bash(__NEW_LINE__ export OPENAI_BASE_URL=\"http://127.0.0.1:8020/v1\")",
      "Bash(export OPENAI_API_KEY=\"dummy\")",
      "Bash(__NEW_LINE__ cd /workspace/petri)",
      "Bash(inspect eval:*)",
      "Bash(python /workspace/olmo/filter_petri_instructions.py:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(python:*)",
      "Bash(ls:*)",
      "Bash(xargs:*)",
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Bash(inspect info:*)",
      "Bash(PYTHONPATH=/workspace/olmo inspect eval:*)",
      "Bash(PYTHONPATH=/workspace/olmo python:*)",
      "Bash(pgrep:*)",
      "Bash(bash run_sycophancy_evals.sh:*)",
      "Bash(nvidia-smi:*)",
      "Bash(fuser:*)",
      "Bash(ps:*)",
      "Bash(kill:*)",
      "Bash(git -C /workspace/olmo log:*)",
      "WebFetch(domain:github.com)",
      "Bash(printf:*)",
      "Bash(tput:*)",
      "Bash(grep:*)",
      "WebSearch",
      "Bash(head:*)",
      "Bash(pip show:*)",
      "Bash(OPENAI_API_KEY=dummy python:*)",
      "Bash(OPENAI_API_KEY=dummy HF_TOKEN=dummy python:*)",
      "Bash(uv add:*)",
      "Bash(uv run python:*)",
      "Bash(bash -n:*)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "Bash(./run_dpo_evals.sh:*)",
      "Bash(export PYTHONPATH=/workspace/olmo:$PYTHONPATH:*)",
      "Bash(vllm serve --help:*)",
      "Bash(unzip:*)",
      "Bash(find:*)",
      "Bash(inspect --help:*)",
      "Bash(inspect score --help:*)",
      "Bash(inspect score:*)",
      "Bash(for bias in positive_bias negative_bias ownership_bias disownership)",
      "Bash(do echo -n \"$bias: \")",
      "Bash(done)",
      "Bash(make style:*)",
      "Bash(make quality:*)",
      "Bash(CUDA_VISIBLE_DEVICES=0,1,2 bash:*)",
      "Bash(cd:*)",
      "Bash(git show:*)"
    ]
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "orchestra-mcp"
  ],
  "hooks": {
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "for t in /dev/pts/*; do [ -w \"$t\" ] && printf '\\a' > \"$t\" 2>/dev/null; done; true"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "for t in /dev/pts/*; do [ -w \"$t\" ] && printf '\\a' > \"$t\" 2>/dev/null; done; true"
          }
        ]
      }
    ]
  }
}
